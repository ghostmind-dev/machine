{
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.gitignore": false,
    "**/.git": false,
    "**/.terraform": false,
    "**/.tfstate": false,
    "**/.npmrc": false,
    "**/dev/dvc-command": false,
    "**/.env.backup": false,
    "**/.dockerignore": false,
    "**/dvc-command": false,
    "**/gha**": false,
    "**/package-lock.json": false,
    "**/node_modules": false,
    "**/__pycache__": false,
    "**/.next": false,
    "**/dist": false,
    "**/.terraform.lock.hcl": false,
    "**/backend.tf": false,
    "**/versions.tf": false,
    "**/.npmignore": false,
    "**/.env.compose": false
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "prettier.singleQuote": true,
  "vs-kubernetes": {
    "disable-linters": ["resource-limits"]
  }
}
